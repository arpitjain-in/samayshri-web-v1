<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600">
  <!-- Industrial flour mill machinery background -->
  <defs>
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f5f5f5;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e0e0e0;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="yellowGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#ffd700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f1b70a;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bgGrad)"/>
  
  <!-- Factory structure -->
  <rect x="50" y="100" width="700" height="400" fill="#d0d0d0" stroke="#999" stroke-width="2"/>
  
  <!-- Yellow hoppers (grain storage) -->
  <path d="M 150 200 L 200 200 L 220 300 L 130 300 Z" fill="url(#yellowGrad)" stroke="#d4a506" stroke-width="2"/>
  <path d="M 300 200 L 350 200 L 370 300 L 280 300 Z" fill="url(#yellowGrad)" stroke="#d4a506" stroke-width="2"/>
  <path d="M 450 200 L 500 200 L 520 300 L 430 300 Z" fill="url(#yellowGrad)" stroke="#d4a506" stroke-width="2"/>
  
  <!-- Machinery base -->
  <rect x="120" y="300" width="100" height="120" fill="#bbb" stroke="#888" stroke-width="2"/>
  <rect x="270" y="300" width="100" height="120" fill="#bbb" stroke="#888" stroke-width="2"/>
  <rect x="420" y="300" width="100" height="120" fill="#bbb" stroke="#888" stroke-width="2"/>
  
  <!-- Pipes and ducts -->
  <rect x="600" y="150" width="80" height="350" fill="#aaa" stroke="#777" stroke-width="2"/>
  <circle cx="640" cy="175" r="35" fill="#999" stroke="#666" stroke-width="2"/>
  
  <!-- Industrial gears/cogs -->
  <circle cx="640" cy="350" r="40" fill="none" stroke="#666" stroke-width="4"/>
  <circle cx="640" cy="350" r="25" fill="#999"/>
  
  <!-- Text label -->
  <text x="400" y="550" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#666" text-anchor="middle">
    Production Floor - Plant 1
  </text>
</svg>
